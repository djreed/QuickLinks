{
	"manifest_version": 2,
  "version": "0.1",
	"name": "QuickLinks",
	"description": "Speed up link access via cached AI web crawling",
  "content_scripts":
  	[
  		{
  			"matches": ["*://*/*"],
  			"all_frames": true,
  			"js":
        [
          "quicklink.js"
        ],
  			"run_at": "document_end"
  		}
  	]
}